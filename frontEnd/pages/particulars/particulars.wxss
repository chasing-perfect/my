}
.crowdordering .pinPeople .every .pRight .cha{
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  margin-right:28rpx;
  align-items: center;
}
.crowdordering .pinPeople .every .pRight .cha .chaPerson{
  font-size:28rpx;
