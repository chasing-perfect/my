/* pages/particulars/particulars.wxss */
.particulars{
  width:100%;
  background-color: #f8f8f8;
}
/* 头部开始 */
.particulars .title-img{
  overflow:hidden;
  position:relative;
  font-family:"MicrosoftYaHei";
  color:#000;
}
.particulars .title-img image{
  width:100%;
  height:752rpx;
  background:#f00;

}
.particulars .title-img .top{
  box-sizing:border-box;
  position:absolute;
  top:44rpx;
  left:0;
  width:100%;
  height:76rpx;
  display: flex;
  justify-content: space-between;
  padding:0 16rpx;

}
.particulars .title-img .top .diff{
   display: inline-block;
   width:76rpx;
    height:76rpx;
    opacity: 0.6;
    border-radius:50%;
    color:#fff;
    text-align: center;
    line-height: 76rpx;
    font-size:16rpx;
    background-color:#1f1f1f;

}
.particulars .title-img .top .part{
  display: flex;
  justify-content: space-between;
  width:178rpx;
  height:100%;

}
.particulars .title-img .top .part .moreMeg{
  position:relative;
  width:76rpx;
  height:76rpx;

}
.particulars .title-img .top .part .moreMeg .megNum{
  position: absolute;
  top:0;
  left:50%;
  transform:translateX(-50%);
  width:53rpx;
	height:38rpx;
	border-radius:10rpx;
	background-color:"#fe4330";
}
.particulars .title-img .bottom{
  position:absolute;
  bottom:42rpx;
  left:50%;
  display: flex;
  justify-content: space-between;
  transform:translateX(-50%);
  width:191rpx;
  height:48rpx;

}
.particulars .title-img .bottom text{
  width:91rpx;
  height:100%;
  border-radius:9;
	background-color: "#ff3000";
  font-size:30rpx;
  text-align: center;
  line-height: 48rpx;
  color:#ffff;
}
/* 头部结束 */
/* 信息开始 */
.particulars .megs{
  box-sizing: border-box;
  padding:22rpx 41rpx 16rpx 25rpx;
  width:100%;
  background-color: #fff;

}
.particulars .megs .meger{
  display: flex;
  justify-content: space-between;
  width:100%;
  font-size: 30rpx;
	line-height: 32rpx;
	color: "#404040";
}
.particulars .megs .meger .txt{
  width:554rpx;
}
.particulars .megs .price{
  margin:25rpx 0 27rpx 0;
  font-size: 34rpx;
	line-height: 20rpx;
	color:#fa380a;
}
.particulars .allNum{
  display: flex;
  font-size: 26rpx;
	line-height: 24rpx;
	color:#505050;
}
.particulars .allNum .kuai{
  margin-right:144rpx;
}
/* 拼单开始 */
.crowdordering{
  background-color: "#f8f8f8";
  overflow: hidden;

}
.crowdordering .shop{
  display: flex;
  justify-content: space-between;
  align-items: center;
  box-sizing: border-box;
  background:#fff;
  padding:23rpx 47rpx 27rpx 16rpx;
  width:100%;
  margin-top:20rpx;
    
}
.crowdordering .shop .left .allQuan{
  display: flex;
  
}
.crowdordering .shop .left .allQuan .quan{
    display: flex;

    width:123rpx;
    height:24rpx;
    border:1rpx dashed #f00;

}
.crowdordering .shop .left .allQuan .quan:nth-child(1){
  margin-bottom:15rpx;
}
.crowdordering .shop .left .allQuan .quan text{
  flex:1;
  text-align:center;
  font-size: 16rpx;
	line-height: 24rpx;
	color:"#ff3000";

}
.crowdordering .shop .left .sq{
  font-size:26rpx;
	line-height:28rpx;
	color: #434343;
  margin-left:10rpx;
}
.crowdordering .shop .right{
  color:#b1b1b1;
}
.crowdordering .shipping-information{
  display: flex;
  margin-top:10rpx;
  height:69rpx;
  padding:0 47rpx 0 16rpx;
   background-color: #fff;
}

 .crowdordering .shipping-information .jian{
    height:69rpx;
    line-height:69rpx;
    color:#b1b1b1;

}
.crowdordering .shipping-information .inforLeft{
  display: flex;
  align-items: center;
  flex:1;
  font-size:24rpx;
	color:#505050;
  
}
.crowdordering .shipping-information .inforLeft text{
  flex:1;
  text-align:center;
  line-height:69rpx;
}
.crowdordering .shipping-information .inforLeft text:nth-child(1)::before{
  display: inline-block;
  width:16rpx;
  height:69rpx;
  background:#f00;
} 



.crowdordering .pinPeople{
  margin-top:10rpx;
  width:100%;
  background-color: #fff;
  
}
.crowdordering .pinPeople .every{
  box-sizing:border-box;
