  height:97rpx;
  width:100%;
  background:#fff;
}
.particulars .my-nav view{
  display: flex;
  flex-direction: column;
  justify-content:center;
  align-items: center;
  font-size:16rpx;
}

 .particulars .my-nav view:nth-child(1),.particulars .my-nav view:nth-child(2),.particulars .my-nav view:nth-child(3){
  flex:1;

  color:#7f7f7f;
} 


.particulars .my-nav view .im{
  margin-bottom:3rpx;

}

.particulars .my-nav .toPing{
  width:249rpx;
  background:#ffcec9;
  text-align: center;
  color:#fff;
  font-size:28rpx;

}
.particulars .my-nav .oneBuy{
  width:237rpx;
  background:#fb3609;
  text-align: center;
  color:#fff;
  font-size:28rpx;
}
/* 模态框开始 */  
.selectType{
  width:100%;

}
.selectType .title-img{
  position:relative;
