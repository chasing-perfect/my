  justify-content: space-between;
  align-items: center;
  width:100%;
  padding:32rpx 46rpx 42rpx 26rpx;

}
 .crowdordering .pinPeople .every .pLeft{
  font-size:28rpx;
  color:#000;


} 
.crowdordering .pinPeople .every .pLeft .allPer{
  font-size:24rpx;
}
.crowdordering .pinPeople .every .pLeft .ig{
 display: flex;
 align-items: center;
  margin-top:15rpx;
  height:71rpx;

}
.crowdordering .pinPeople .every .pLeft .ig image{
  width:71rpx;
  height:71rpx;
  margin-right:20rpx;

}
.crowdordering .pinPeople .every .pRight{
