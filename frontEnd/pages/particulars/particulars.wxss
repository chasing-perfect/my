  height:120rpx;
  border-radius: 10rpx;
 
}

.selectType .title-img .priceAndSize{
  padding-left:10rpx;
  flex-direction: column;
  justify-content: space-around;
  position:relative;
  flex:1;
  font-size:16rpx;
}
.selectType .title-img .priceAndSize .pr{
  color:#f00;
}

.selectType .title-img .priceAndSize .clone{
  position: absolute;
  right:20rpx;
