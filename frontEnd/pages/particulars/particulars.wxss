  justify-content: space-between;
  font-size:16rpx;
  margin-top:15rpx;

}
.shopMore .shop-list .list-every .shop-price .surePrice{
  color:#f00;
  
}
.shopMore .shop-list .list-every .shop-price .toGo{
  display: flex;
  align-items: center;
  justify-content: space-around;
  
  font-size:12rpx;
