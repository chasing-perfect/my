}
.crowdordering .pinPeople .every .pRight .goPing{
  width:135rpx;
	height:60rpx;
	border-radius:10rpx;
	background-color: #fb3609;
  font-size:32rpx;
  color:#fff;
  text-align:center;
  line-height:60rpx;
}
.crowdordering .appraise{
  box-sizing:border-box;
  width:100%;
  margin-top:20rpx;
  background:#fff;
}
.crowdordering .appraise .shopAppraise{
  display: flex;
  justify-content: space-between;
  padding:39rpx 27rpx;
  font-size:36rpx;
	color: #3c3c3c;
   background-color: #fff;

}
.crowdordering .appraise .shopAppraise .more{
  display: flex;
  align-items: center;
  font-size:36rpx;
  color: #b1b1b1;
}
.crowdordering .appraise .selectLook{
  display: flex;
  flex-wrap:wrap;
  padding:0 27rpx;
  background-color: #fff;
  
}
.crowdordering .appraise .selectLook view{
	border-radius:20rpx;
	background-color: #fcd8d1;  
  font-size:30rpx;
  text-align: center;
  line-height:50rpx;
  padding:13rpx 21rpx 8rpx 14rpx;
  margin-bottom:22rpx;
   margin-right:15rpx;
}
.crowdordering .appraise .allList{
  box-sizing: border-box;
  width:100%;
  background-color: #f8f8f8;
 
}
.crowdordering .appraise .allList .list{
  margin-bottom:20rpx;
  padding:23rpx 27rpx;
   background-color: #fff;
}
.crowdordering .appraise .allList .list .per{
  display: flex;
  justify-content:flex-start;
  align-items: center;
}
.crowdordering .appraise .allList .list .per image{
  width:56rpx;
  height:56rpx; 
  background:pink;
  border-radius: 50%;
  margin-right:15rpx;
}
.crowdordering .appraise .allList .list .wangName{
  font-size:23rpx;
  margin-left:10rpx;
  line-height:56rpx;

}
.crowdordering .appraise .allList .list .content{
  font-family: "SimHei";
	font-size:30rpx;
	line-height:44rpx;
  margin-top:15rpx;
	color: #2a2929;

}
.crowdordering .appraise .allList .watchMore{
  height:78rpx;
  font-size:30rpx;
  color: #b1b1b1;
  text-align: center;
  line-height:78rpx;
  margin-bottom:10rpx;
  background:#fff;
}
/* 商品详情开始 */
.shopMore{
  width:100%;
}
.shopMore .shop-title{
  width:100%;
  height:100rpx;
  font-size:36rpx;
	line-height:100rpx;
	color:#111111;
